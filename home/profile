export PATH=$HOME/bin:/usr/local/bin:/usr/local/sbin:$PATH

if [ "$(hostname)" = "jreggio-wsl" ]; then
  export P4CONFIG=p4config
  export JAVA_HOME=$HOME/dev/tools/Linux/jdk/jdk1.6.0_21_x64
  export ORACLE_HOME=$HOME/dev/tools/Linux/oracle/10.1.0.2
  export LD_LIBRARY_PATH=$ORACLE_HOME/client/lib
  export PATH=$PATH:$JAVA_HOME/bin:$ORACLE_HOME/client/lib:$ORACLE_HOME/client/bin:$ORACLE_HOME/bin
fi
